-- Date: August 2025

-- Clear existing data (except system user)
DELETE FROM leaderboard WHERE user_id > 1;
DELETE FROM words WHERE game_id IN (SELECT game_id FROM games WHERE user_id > 1);
DELETE FROM letters WHERE game_id IN (SELECT game_id FROM games WHERE user_id > 1);
DELETE FROM games WHERE user_id > 1;
DELETE FROM settings WHERE user_id > 1;
DELETE FROM users WHERE user_id > 1;

