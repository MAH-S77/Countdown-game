-- Display completion message
SELECT 'Sample data inserted successfully!' as Status;
SELECT 'Total Users Created: ' as Info, COUNT(*) as Count FROM users WHERE user_id > 1;
SELECT 'Total Games Created: ' as Info, COUNT(*) as Count FROM games;
SELECT 'Total Words Created: ' as Info, COUNT(*) as Count FROM words;
SELECT 'Database ready for testing!' as Final_Status;
